{
  "name": "template-node-typescript-express",
  "version": "1.0.0",
  "description": "Starter template for use typescript and express in node",
  "main": "./dist/main.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/main.js",
    "build": "tsc -p ./tsconfig.json",
    "dev": "tsx --watch ./src/main.ts",
    "format": "prettier ./src --ignore-unknown --write"
  },
  "keywords": [
    "node",
    "typescript",
    "template",
    "express",
    "nodejs",
    "sqlite"
  ],
  "author": "LiasCode",
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "compression": "^1.8.0",
    "cookie-parser": "1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "helmet": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.28",
    "@types/uuid": "^9.0.8",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
